{
  "name": "timepicker.js",
  "version": "2.2.0",
  "description": "A lightweight, customizable, TimePicker. Zero dependencies.",
  "author": "Jonatas Walker",
  "homepage": "https://github.com/jonataswalker/timepicker.js",
  "license": "MIT",
  "main": "dist/timepicker.js",
  "keywords": [
    "timepicker",
    "time",
    "picker"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "run-s lint build:css rollup:dev",
    "build": "run-s lint rollup build:css",
    "build:css": "node build/build-css",
    "rollup": "rollup -c build/config.js",
    "rollup:dev": "rollup -w -c build/config.js",
    "lint": "eslint build test src examples --cache",
    "test": "run-s build test:e2e",
    "test:e2e": "casperjs test test/spec --includes=test/include.js --ssl-protocol=any --ignore-ssl-errors=true"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonataswalker/timepicker.js.git"
  },
  "bugs": {
    "url": "https://github.com/jonataswalker/timepicker.js/issues"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "boxen": "^1.0.0",
    "bytes": "^3.0.0",
    "casperjs": "^1.1.3",
    "chalk": "^2.3.2",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-jwalker": "^5.1.0",
    "gzip-size": "^4.1.0",
    "node-sass": "^4.4.0",
    "node-sass-json-importer": "^3.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.21",
    "postcss-import": "^11.1.0",
    "postcss-reporter": "^5.0.0",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "stylelint": "^9.2.0",
    "susy": "^3.0.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {}
}
